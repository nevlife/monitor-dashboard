# React Monitor Dashboard Environment Configuration

# Django API Backend URL
REACT_APP_API_BASE_URL=http://localhost:8000/api

# Update intervals (in milliseconds)
REACT_APP_UPDATE_INTERVAL=5000
REACT_APP_CHART_UPDATE_INTERVAL=30000

# Development settings
REACT_APP_DEBUG=true

# Production settings (uncomment for production)
# REACT_APP_API_BASE_URL=https://api.your-domain.com/api
# REACT_APP_UPDATE_INTERVAL=10000
# REACT_APP_DEBUG=false